<!doctype html>
<html class="fixed">
	{% include 'company/admin.header.tmp' %}
	<body>
		<section class="body">
			{% include 'company/admin.header.right.tmp' %}
			<!-- end: header -->

			<div class="inner-wrapper">
					<!-- start: sidebar -->
					{% include 'company/company.admin.sidebar.tmp' %}
					<!-- end: sidebar -->
	
					<section role="main" class="content-body">
						<header class="page-header">
							<h2>Do you really want to remove this Job?</h2>
						
						</header>
	
						<!-- start: page -->
						
	
						
	
						<div class="row">
							
							<div class='col-lg-8'>
								
									<section class="panel">
											<div class="panel-body panel-body-nopadding">
												<div class="owl-carousel mb-md owl-theme owl-carousel-init" data-plugin-carousel="" data-plugin-options="{ &quot;items&quot;: 1, &quot;autoHeight&quot;: true }" style="display: block; opacity: 1;">
													
													
													
													<div class="owl-controls clickable">
														<div class="owl-pagination"><div class="owl-page"><span class=""></span></div>
															<div class="owl-page"><span class=""></span></div>
															<div class="owl-page active"><span class=""></span></div>
														</div>
													</div>
												</div>
		
												<div class="p-md">
													<h4 class="text-semibold mt-none"><a style="color:#55a747"  href='/jobs/job-{{job.pk}}/'>{{ job.title }}</a></h4>
													<p>{{ job.summary|safe  }}</p>
												</div>
											</div>
											<div class="panel-footer panel-footer-btn-group">
												
												<a style="font-size:22px; font-weight: 700; color:#a74747"  href="/company/admin/confirm-delete/job-{{job.pk}}/confirmed/"><i class="fa fa-check-square-o mr-lg"></i> YES</a>
												<a style="font-size:22px; font-weight: 700; color:#55a747"  href="/company/admin/jobs/"><i class="fa fa-times-circle mr-lg"></i> NO</a>
											</div>
									</section>
									
									</div>
									
							<!-- End of list jobs -->
							</div>
							
						</div>
						<!-- end: page -->
					</section>
				</div>

		</section>

	{% include 'company/admin.footer.tmp' %}