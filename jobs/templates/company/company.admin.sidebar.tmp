<aside id="sidebar-left" class="sidebar-left">
					
						<div class="sidebar-header">
							<div class="sidebar-title">
								Navigation
							</div>
							<div class="sidebar-toggle hidden-xs" data-toggle-class="sidebar-left-collapsed" data-target="html" data-fire-event="sidebar-left-toggle">
								<i class="fa fa-bars" aria-label="Toggle sidebar"></i>
							</div>
						</div>
					
						<div class="nano">
								<div class="nano-content">
									<nav id="menu" class="nav-main" role="navigation">
										<ul class="nav nav-main">
											<li class="{{ sidebar_profile_info_active }}">
												<a href="/company/admin/">
													<i class="fa fa-home" aria-hidden="true"></i>
													<span> Company Profile </span>
												</a>
											</li>
											<li class="{{ sidebar_create_job_active }}">
													<a href="/company/admin/create-job/">
															<i class="fa fa-plus" aria-hidden="true"></i> Post Jobs & Internships
													</a>
												</li>
												
												<li class="{{ sidebar_job_listing_active }}">
													<a href="/company/admin/jobs/">
														<i class="fa fa-list" aria-hidden="true"></i> Posted Jobs
													</a>
												</li>

												<li class="{{ sidebar_job_saved_active }}">
														<a href="/company/admin/jobs/drafts/">
															<i class="fa fa-mail-reply-all" aria-hidden="true"></i>Saved Jobs
														</a>
													</li>


											<li class="{{ sidebar_applications_active }}">
												<a href="/company/admin/applications/">
													{% if unread_applications_count %}
													<span class="pull-right label label-primary">{{ unread_applications_count }}</span>
													{% endif %}
													<i class="fa fa-envelope" aria-hidden="true"></i>
													<span>Applications</span>
												</a>
											</li>

											<li class="{{ sidebar_messages_active }}">
													<a href="/company/admin/messages/">
														{% if unread_messages_count %}
														<span class="pull-right label label-primary">{{ unread_messages_count }}</span>
														{% endif %}
														<i class="fa fa-envelope" aria-hidden="true"></i>
														<span>Messages</span>
													</a>
												</li>


											<li class="{{ sidebar_recruit_active }}">
												<a href="/company/recruit/">
													<i class="fa fa-paper-plane-o" aria-hidden="true"></i>Employee DB
												</a>
											</li>

										
											<li class="">
													<a href="/logout/">
														<i class="fa fa-power-off" aria-hidden="true"></i>

														<span>Logout</span>
													</a>
												</li>

										</ul>
									</nav>
						
								</div>
						
						</div>
					
					</aside>