	<!DOCTYPE html>
	<html lang="zxx" class="no-js">
	<head>
		<!-- Mobile Specific Meta -->
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<!-- Favicon-->
		<link rel="shortcut icon" href="img/elements/fav.png">
		<!-- Author Meta -->
		<meta name="author" content="colorlib">
		<!-- Meta Description -->
		<meta name="description" content="">
		<!-- Meta Keyword -->
		<meta name="keywords" content="">
		<!-- meta character set -->
		<meta charset="UTF-8">
		<!-- Site Title -->
		<title>Job Listing</title>

		<link href="https://fonts.googleapis.com/css?family=Poppins:100,200,400,300,500,600,700" rel="stylesheet"> 
			<!--
			CSS
			============================================= -->
			<link rel="stylesheet" href="/static/css/linearicons.css">
			<link rel="stylesheet" href="/static/css/owl.carousel.css">
			<link rel="stylesheet" href="/static/css/font-awesome.min.css">
			<link rel="stylesheet" href="/static/css/nice-select.css">			
			<link rel="stylesheet" href="/static/css/magnific-popup.css">
			<link rel="stylesheet" href="/static/css/bootstrap.css">
			<link rel="stylesheet" href="/static/css/main.css">
		</head>
		<body>
			{% include 'header.tmp' %}

			<!-- start banner Area -->
			<section class="banner-area relative" id="home">	
				<div class="overlay overlay-bg"></div>
				<div class="container">
					<div class="row d-flex align-items-center justify-content-center">
						<div class="about-content col-lg-12">
							<h1 class="text-white">
								Company Sign Up			
							</h1>	
							<p class="text-white link-nav"><a href="index.html">Home </a>  <span class="lnr lnr-arrow-right"></span>  <a href=""> Create New Account</a></p>
						</div>											
					</div>
				</div>
			</section>
			<!-- End banner Area -->	

			
			<!-- Start Align Area -->
			<div class="whole-wrap">
				<div class="container">
					
					
					<div class="section-top-border">
						<div class="row">
							<div class="col-lg-12 col-md-8">
								<h3 class="mb-30">Sign Up</h3>
								{% if suc %}
									<div class='txt-succes'> {{ suc }} </div>
								{% endif %}
								{% if error %}
									<div class='txt-succes'> {{ error }} </div>
								{% endif %}

								<form action="/company-signup/" enctype="multipart/form-data" method="POST">
									{% csrf_token %}
									<div class="input-group-icon mt-10">
										<div class="icon"><i class="fa fa-pencil" aria-hidden="true"></i></div>
												
										{{ company_form.name }}
									</div>


									<div class="input-group-icon mt-10">
											<div class="icon"><i class="fa fa-user" aria-hidden="true"></i></div>
													
											{{ user_form.username }}
									</div>
									<div class="input-group-icon mt-10">
											<div class="icon"><i class="fa fa-key" aria-hidden="true"></i></div>
													
											{{ user_form.password }}
									</div>
									<div class="input-group-icon mt-10">
											<div class="icon"><i class="fa fa-envelope" aria-hidden="true"></i></div>
											{{ user_form.email }}
									</div>

									<div class="input-group-icon mt-10">
											<div class="icon"><i class="fa fa-globe" aria-hidden="true"></i></div>	
											{{ company_form.website }}
									</div>

									
									<div class="input-group-icon mt-10">
										<div class="icon"><i class="fa fa-phone" aria-hidden="true"></i></div>
										{{ company_form.phone }}
									</div>

									<div class="input-group-icon mt-10">
											<div class="icon"><i class="fa fa-plane" aria-hidden="true"></i></div>
											{{ company_form.city }}		
									</div>

									<div class="input-group-icon mt-10">
										<div class="icon"><i class="fa fa-globe" aria-hidden="true"></i></div>
										
										<div class="form-select" id="default-select">
												{{ company_form.region }}
										</div>
									</div>
									
									<div class="mt-10">
											{{ company_form.brief_description }}
									</div>
									<div class="input-group-icon mt-10">
										<label>Profile Picture</label><br />
										{{ company_form.profile_pic }}
											
									</div>
									<div class="mt-10">
										<button type="submit" class="genric-btn info">Create Account</button>
									</div>
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- End Align Area -->

		{% include 'footer.tmp' %}	
		</body>
	</html>