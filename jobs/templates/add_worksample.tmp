
<form method='POST' action="/employee/build-resume/worksample/" enctype="multipart/form-data">
    {% csrf_token %}
    <div class="mt-10">
            <label>Name </label>
        {{ workSampleForm.name }}
    </div>

    <div class="mt-10">
            <label>Description </label>
        {{ workSampleForm.description }}
    </div>

    <div class="mt-10">
            <label>Date </label>
        {{ workSampleForm.date }}
    </div>

    <div class="mt-10">
            <label>Document </label><br />
        {{ workSampleForm.document }}
    </div>


    <div class='form-group'>
        <hr />
        <button type="submit" class="genric-btn skill-tags">Save and Continue </button>
        <a href='/employee/' class="genric-btn skill-tags">Cancel </a>    </div>
</form>

