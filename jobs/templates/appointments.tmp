    <body>
        {% include 'header.tmp' %}

        <!-- start banner Area -->
        <section class="banner-area relative" id="home">	
            <div class="overlay overlay-bg"></div>
            <div class="container">
                <div class="row d-flex align-items-center justify-content-center">
                    <div class="about-content col-lg-12">
                        <h1 class="text-white">
                            Make Appointment 
                        </h1>	
                        <p class="text-white link-nav"><a href="index.html">Home </a>  <span class="lnr lnr-arrow-right"></span>  <a href=""> Appointments </a></p>
                    </div>											
                </div>
            </div>
        </section>
        <!-- End banner Area -->	

        
        <!-- Start Align Area -->
        <div class="whole-wrap">
            <div class="container">
                
                
                <div class="section-top-border">
                    <div class="row">
                        <div class="col-lg-12 col-md-8">
                            <h3 class="mb-30">Appointments </h3>
                            
                            <table class="table table-bordered table-striped">
                                <tr><th>Needs help with</th><th>Time slot</th><th>Date</th><th>Status</th></tr>
                                {% for appointment in appointments %}
                                <tr><td>{{ appointment.need }}</td><td>{{ appointment.slot}}</td><td>{{ appointment.date}}</td>
                                    
                                    <td>{{ appointment.get_status_display }}</td>
                                </tr>
                                {% endfor %}
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Align Area -->

        <!-- start footer Area -->		
        <footer class="footer-area section-gap">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3  col-md-12">
                        
                    </div>
                    <div class="col-lg-6  col-md-12">
                        <div class="single-footer-widget newsletter">
                            <h6>Newsletter</h6>
                            <p>You can trust us. we only send promo offers, not a single spam.</p>
