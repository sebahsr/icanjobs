
<form method='POST' action="/employee/build-resume/experience/">
    {% csrf_token %}
    <div class="mt-10">
        <label>Job Title</label>
        {{ experienceForm.job_title }}
    </div>

    <div class="mt-10">
        <label>Company Name</label>
        {{ experienceForm.company_name }}
    </div>
    
    <div class="mt-10">
        <label>City</label>
        {{ experienceForm.city }}
    </div>
    
    <div class="input-group-icon mt-10">
        <label>Region</label>
        <div class="form-select" id="default-select">
            {{ experienceForm.region }}
        </div>
    </div>

    <div class="form-group">

        <label>Start Date</label>
        {{ experienceForm.start_month}} 
        

        {{ experienceForm.start_year}}
    </div>
    <div class="mt-10">
        <label>End Date</label>
            {{ experienceForm.end_month}} 
            {{ experienceForm.end_year}} 
    </div>

    <div class="mt-10">
        <hr />
        I currently work here {{ experienceForm.is_currently}}
    </div>
    <div class="mt-10">
        <hr />
        {{ experienceForm.description }}
    </div>

    <div class='form-group'>
        <hr />
        <button type="submit" class="genric-btn primary-border">Save Changes</button>
    </div>
</form>