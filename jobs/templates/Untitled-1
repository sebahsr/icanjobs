UPDATE wp_options SET option_value = replace(option_value, 'http://142.93.98.155', 'http://sportzoneet.com/') WHERE option_name = 'home' OR option_name = 'siteurl';

UPDATE wp_posts SET guid = replace(guid, 'http://142.93.98.155','http://sportzoneet.com/');

UPDATE wp_posts SET post_content = replace(post_content, 'http://142.93.98.155', 'http://sportzoneet.com/');

UPDATE wp_postmeta SET meta_value = replace(meta_value,'http://142.93.98.155','http://sportzoneet.com/');