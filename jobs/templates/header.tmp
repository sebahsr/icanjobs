                          <li class="menu-has-children"><a href="/company/admin/login/">Login</a></li> 
                          <li class="menu-has-children"><a href="/company/admin/signup/">Register</a></li> 

                          {% endif %}

                      </ul>
                    </li>
                    <li class="menu-has-children"><a href="/blogs/">Career Advice</a></li>
                    <li class="menu-has-children"><a href="/news/">News</a></li>


                    <li class="menu-has-children"><a href="">Employment solutions <i class="fa fa-caret-down"></i></a>
