<form method="POST" enctype="multipart/form-data" action="/employee/build-resume/general/">
    {% csrf_token %}
    <div class="mt-10">
        <label>First Name</label>
            {{ userForm.first_name }}
    </div>

    <div class="mt-10">
            <label>Last Name</label>

            {{ userForm.last_name }}
    </div>

    <div class="mt-10">
            <label>Username</label>

            {{ userForm.username }}
    </div>


    <div class="input-group-icon  mt-10">
            <label>Email</label>

        {{ userForm.email }}
    </div>



    <div class="input-group-icon  mt-10">
            <label>Phone</label>

        {{ employeeForm.phone }}
    </div>


    <div class="input-group-icon  mt-10">
        <label>Location</label>        
        {{ employeeForm.city }}
    </div>    
    <div class="input-group-icon mt-10">
        <label>Region</label>
        <div class="form-select" id="default-select">
            {{ employeeForm.region }}
        </div>
    </div>

    <div class="input-group-icon mt-10">
        <label>Facebook</label>
        <div class="form-select" id="default-select">
            {{ employeeForm.facebook_url }}
        </div>
    </div>
    
    <div class="input-group-icon mt-10">
        <label>Linkedin</label>
        <div class="form-select" id="default-select">
            {{ employeeForm.linkedin_url }}
        </div>
    </div>
    
    <div class="input-group-icon mt-10">
        <label>Twitter</label>
        <div class="form-select" id="default-select">
            {{ employeeForm.twitter_url }}
        </div>
    </div>
  

    <div class="mt-10">
        <p><label>Profile Pic</label></p>
        {{ employeeForm.profile_pic }}
    </div>


    <div class='form-group'>
        <hr /><button type="submit" class="genric-btn skill-tags">Save and Continue </button>
        <a href='/employee/' class="genric-btn skill-tags">Cancel </a>
    </div>

</form>
