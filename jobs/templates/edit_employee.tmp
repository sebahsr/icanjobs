<form method="POST" enctype="multipart/form-data" action="/employee/edit/general/">
    {% csrf_token %}
    <div class="mt-10">
        <label>First Name</label>
            {{ userForm.first_name }}
    </div>

    <div class="mt-10">
            <label>Last Name</label>

            {{ userForm.last_name }}
    </div>

    <div class="mt-10">
            <label>Username</label>

            {{ userForm.username }}
    </div>


    <div class="input-group-icon  mt-10">
            <label>Email</label>

        {{ userForm.email }}
    </div>



    <div class="input-group-icon  mt-10">
            <label>Phone</label>

        {{ employeeForm.phone }}
    </div>


    <div class="input-group-icon  mt-10">
        <label>City</label>        
        {{ employeeForm.city }}
    </div>    
    <div class="input-group-icon mt-10">
        <label>Region</label>
        <div class="form-select" id="default-select">
            {{ employeeForm.region }}
        </div>
    </div>

    <div class="input-group-icon mt-10">
        <label>Facebook</label>
        <div class="form-select" id="default-select">
            {{ employeeForm.facebook_url }}
        </div>
    </div>
    
    <div class="input-group-icon mt-10">
        <label>Linkedin</label>
        <div class="form-select" id="default-select">
            {{ employeeForm.linkedin_url }}
        </div>
    </div>
    
    <div class="input-group-icon mt-10">
        <label>Twitter</label>
        <div class="form-select" id="default-select">
            {{ employeeForm.twitter_url }}
        </div>
    </div>
    
    <div class="mt-10">
        <label>Profile Summary </label>
        {{ employeeForm.about_me }}
    </div>

    <div class="mt-10">
            <label>Volunteer Experience/Membership/Affilations</label>
            {{ employeeForm.volunteer_experience }}
    </div>

    <div class="mt-10">
        <p><label>Profile Pic</label></p>
        {{ employeeForm.profile_pic }}
    </div>


    <div class='form-group'>
        <hr />
        <button type="submit" class="genric-btn primary-border">Save Changes</button>
    </div>

</form>
