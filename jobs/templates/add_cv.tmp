
<form method='POST' action="/employee/edit/cv/" enctype="multipart/form-data">
    {% csrf_token %}
    <div class="mt-10">
            <label>CV Name </label>
        {{ cvForm.name }}
    </div>

    <div class="mt-10">
            <label>CV Document  </label> <br />
        {{ cvForm.document }}
    </div>

    <div class='form-group'>
        <hr />
        <button type="submit" class="genric-btn primary-border">Add CV</button>
    </div>
</form>