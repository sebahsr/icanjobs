
<form method='POST' action="/employee/build-resume/cv/" enctype="multipart/form-data">
    {% csrf_token %}
    <div class="mt-10">
            <label class="resume-label">Your Position title </label>
        {{ cvForm.name }}
    </div>

    <div class="mt-10">
            <label class="resume-label">CV Document  </label> <br />
        {{ cvForm.document }}
    </div>

    <div class='form-group'>
        <hr />
        <button type="submit" class="genric-btn skill-tags">Save and Continue </button>
        <a href='/employee/' class="genric-btn skill-tags">Cancel </a>    </div>
</form>