
<form method='POST' action="/employee/build-resume/worklink/">
    {% csrf_token %}
    <div class="mt-10">
            <label>Name </label>
        {{ workLinkForm.name }}
    </div>

    <div class="mt-10">
            <label>URL </label>
        {{ workLinkForm.url }}
    </div>


    <div class="mt-10">
            <label>Description </label>
        {{ workLinkForm.description }}
    </div>



    <div class='form-group'>
        <hr />

        <button type="submit" class="genric-btn skill-tags">Save and Continue </button>
        <a href='/employee/' class="genric-btn skill-tags">Cancel </a>    </div>    </div>
</form>