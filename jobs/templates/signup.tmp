			<link rel="stylesheet" href="/static/css/bootstrap.css">
			<link rel="stylesheet" href="/static/css/main.css">
		</head>
		<body>
			{% include 'header.tmp' %}

			<!-- start banner Area -->
			<section class="banner-area relative" id="home">	
				<div class="overlay overlay-bg"></div>
				<div class="container">
					<div class="row d-flex align-items-center justify-content-center">
						<div class="about-content col-lg-12">
							<h1 class="text-white">
								Employee Sing Up			
							</h1>	
							<p class="text-white link-nav"><a href="index.html">Home </a>  <span class="lnr lnr-arrow-right"></span>  <a href=""> Create New Account</a></p>
						</div>											
					</div>
				</div>
			</section>
			<!-- End banner Area -->	

			
			<!-- Start Align Area -->
			<div class="whole-wrap">
				<div class="container">
					
					
					<div class="section-top-border">
						<div class="row">
							<div class="col-lg-12 col-md-8">
								<h3 class="mb-30">Sign Up</h3>


								{% if suc %}
									<div class='txt-succes'> {{ suc }} </div>
								{% endif %}
								<form action="/signup/" enctype="multipart/form-data" method="POST">
									{% csrf_token %}
								
									
									<div class='form-group'>	
										{% if user_form.first_name.errors %} 
											<span class='text-danger'>{{  user_form.first_name.errors.0 }}</span>
										{% endif %}
										<div class="input-group-icon mt-10">
											<div class="icon"><i class="fa fa-pencil" aria-hidden="true"></i>
											</div>
											
											{{ user_form.first_name }}
										</div>
									</div>

									<div class='form-group'>	
										{% if user_form.last_name.errors %} 
											<span class='text-danger'>{{  user_form.last_name.errors.0 }}</span>
										{% endif %}
										<div class="input-group-icon mt-10">
												<div class="icon"><i class="fa fa-pencil" aria-hidden="true"></i></div>
												
												{{ user_form.last_name }}
										</div>
									</div>


									<div class='form-group'>	
										{% if userNameForm.username.errors %} 
											<span class='text-danger'>{{  userNameForm.username.errors.0 }}</span>
										{% endif %}
										<div class="input-group-icon mt-10">
											<div class="icon"><i class="fa fa-user" aria-hidden="true"></i></div>
											{{ userNameForm.username }}
										</div>
									</div>

									<div class='form-group'>	
										{% if passerr %} 
											<span class='text-danger'>{{  passerr }}</span>
										{% endif %}
										
										<div class="input-group-icon mt-10">
											<div class="icon"><i class="fa fa-key" aria-hidden="true"></i></div>
											<input type="password" name="pwd" placeholder="New Password" required="" class="single-input">
										</div>
										
									</div>

									<div class='form-group'>	
										{% if passerr %} 
											<span class='text-danger'>{{  passerr }}</span>
										{% endif %}
										
										<div class="input-group-icon mt-10">
											<div class="icon"><i class="fa fa-key" aria-hidden="true"></i></div>
											<input type="password" name="repeat_pwd" placeholder="Repeat New Password" required="" class="single-input">
										</div>
										
									</div>
									
									
									<div class="form-group mt-10">
										<button type="submit" class="genric-btn info">Create Account</button>
									</div>
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- End Align Area -->

			<!-- start footer Area -->		
			<footer class="footer-area section-gap">
				<div class="container">
					<div class="row">
						<div class="col-lg-3  col-md-12">
							
						</div>
						<div class="col-lg-6  col-md-12">
							<div class="single-footer-widget newsletter">
								<h6>Newsletter</h6>
								<p>You can trust us. we only send promo offers, not a single spam.</p>
								<div id="mc_embed_signup">
									<form target="_blank" novalidate="true" action="https://spondonit.us12.list-manage.com/subscribe/post?u=1462626880ade1ac87bd9c93a&amp;id=92a4423d01" method="get" class="form-inline">

										<div class="form-group row" style="width: 100%">
											<div class="col-lg-8 col-md-12">
												<input name="EMAIL" placeholder="Enter Email" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter Email '" required="" type="email">
												<div style="position: absolute; left: -5000px;">
													<input name="b_36c4fd991d266f23781ded980_aefe40901a" tabindex="-1" value="" type="text">
												</div>
											</div> 
										
											<div class="col-lg-4 col-md-12">
												<button class="nw-btn primary-btn">Subscribe<span class="lnr lnr-arrow-right"></span></button>
											</div> 
										</div>		
										<div class="info"></div>
									</form>
								</div>		
							</div>
						</div>
												
					</div>

					<div class="row footer-bottom d-flex justify-content-between">
						<p class="col-lg-8 col-sm-12 footer-text m-0 text-white">
