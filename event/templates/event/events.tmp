<!DOCTYPE html>
<html lang="zxx" class="no-js">
<head>
	<!-- Mobile Specific Meta -->
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<!-- Favicon-->
	<link rel="shortcut icon" href="img/fav.png">
	<!-- Author Meta -->
	<meta name="author" content="codepixer">
	<!-- Meta Description -->
	<meta name="description" content="">
	<!-- Meta Keyword -->
	<meta name="keywords" content="">
	<!-- meta character set -->
	<meta charset="UTF-8">
	<!-- Site Title -->
	<title>Job Listing</title>

	<link href="https://fonts.googleapis.com/css?family=Poppins:100,200,400,300,500,600,700" rel="stylesheet"> 
		<!--
		CSS
		============================================= -->
		<link rel="stylesheet" href="/static/css/linearicons.css">
		<link rel="stylesheet" href="/static/css/font-awesome.min.css">
		<link rel="stylesheet" href="/static/css/bootstrap.css">
		<link rel="stylesheet" href="/static/css/magnific-popup.css">
		<link rel="stylesheet" href="/static/css/nice-select.css">					
		<link rel="stylesheet" href="/static/css/animate.min.css">
        <link rel="stylesheet" href="/static/css/owl.carousel.css">
        
		<link rel="stylesheet" href="/static/css/main-events.css">
		<link rel="stylesheet" href="/static/css/main.css">
	</head>
	<body>

		<!-- header temp -->
			{% include 'header.tmp' %}
		<!-- header temp END -->

		<!--header end here-->

<!--page title section-->
<section class="inner_cover parallax-window" data-parallax="scroll" data-image-src="/static/img/bg/bg-img.png">
    <div class="overlay_dark"></div>
    <div class="container">
        <div class="row justify-content-center align-items-center">
            <div class="col-12">
                <div class="inner_cover_content">
                    <h3>
                        iCAN Events
                    </h3>
                </div>
            </div>
        </div>

    </div>
</section>
<!--page title section end-->


<!--events section -->
<section class="pt100 pb100">
    <div class="container">
        <div class="row justify-content-center">
            <!--blog section start -->
            <div class="col-12 col-md-8">
                <div class="row">
                <style>
                        .event-thumbnail-container {
                            width: 100%;
                            height: 300px;
                            
                            background-size: cover;
                            background-repeat: no-repeat;
                            background-position: 50% 50%;
                        }
                    </style>
                {% for event in events %}
                <div style="height:700px; border-bottom: solid 1px black;" class="col-sm-6">
                    

                    <div style="background-image: url('{{event.image.url}}');" class="event-thumbnail-container"></div>
                    <br />
                    <div class="blog_box_data">
                        
                        <span class="blog_date">
                            <i class="fa fa-calendar"></i> 
                            {{ event.upcoming.days }} 
                            {% if event.event_start_date == event.event_end_date %}
                            {{ event.event_start_date }}
                            {% else %}
                            {{ event.event_start_date }} - {{ event.event_end_date }}
                            {% endif %}
                        </span>
                        <hr />
                        <h5>
                           {{ event.title|title }}
                        </h5>
                            <hr />
                        <p class="blog_word">
                            {{ event.description|striptags | truncatechars:200}}
                        </p>
                        <a class="btn btn-primary" href="/event/event-{{event.pk }}/" class="readmore_btn">Read More</a>
                    </div>
                </div>
                {% empty %}
                <p>No Events Found...</p>
                {% endfor %}
                </div>

                <!--pagenition menu-->
                <div class="mt70 mb70">
                    <ul class="nav_menu">
                            {% if current_page.has_previous %}
                            <li><a href="?page={{ current_page.previous_page_number }}">Previous <span class="lnr lnr-arrow-left"></span> </a></li>
                            {% endif %}
    
                            {% if current_page.has_next %}
                            <li><a href="?page={{ current_page.next_page_number }}">Next <span class="lnr lnr-arrow-right"></span> </a></li>
                            {% endif %}

                    </ul>
                </div>
                <!--pageintion -->
                    
            </div>
            <!--blog section end-->

            <!--sidebar section -->
            {% include 'event/event.sidebar.tmp' %}

            <!--sidebar section end -->

        </div>
    </div>
</section>
<!--event section end -->



<!--get tickets section -->
<section class="bg-img pt100 pb100" style="background-image: url('/static/img/bg/tickets.png');">
    <div class="container">
        <div class="section_title mb30">
            <h3 class="title justify-content-center color-light">
                Make sure you dont miss events
            </h3>
        </div>
    </div>
</section>
<!--get tickets section end-->

<!--footer start -->



		{% include 'join_us_section.tmp' %}		
	   
		<!-- start footer Area -->		
	{% include 'footer.tmp' %}	
	</body>
</html>



